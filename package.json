{
  "name": "node-typescript-express-mongodb",
  "version": "1.0.0",
  "description": "Boilerplate for Node-TypeScript-Express-MongoDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/sksamip/masterpanel-node"
  },
  "engines": {
    "npm": ">=8.0.0 <10.0.0",
    "node": ">=16.0.0 <19.0.0"
  },
  "keywords": [
    "TypeScript",
    "express",
    "nodejs",
    "boilerplate",
    "eslint",
    "mongodb",
    "swagger",
    "openapi",
    "JWT"
  ],
  "author": "samip",
  "scripts": {
    "build:ci": "tsc && tsc-alias",
    "build": "rimraf dist && npm run build:ci",
    "build:prod": "npm run build:dev",
    "build:qa": "npm run build:dev",
    "build:local": "npm run build:dev",
    "dev": "nodemon",
    "local": "NODE_ENV=development.local nodemon",
    "dev:qa": "NODE_ENV=qa nodemon",
    "start": "node dist/src/app.js",
    "start:qa": "NODE_ENV=qa node src/app.js",
    "start:prod": "NODE_ENV=production node src/app.js",
    "start:dev": "NODE_ENV=development node src/app.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "bump": "npm version --no-git-tag-version",
    "style:all": "tsc && tsc-alias && npm run eslint && npm run format",
    "scriptname": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.321.1",
    "@sentry/node": "^7.52.1",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cryptr": "^6.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.5.1",
    "grecaptcha": "^1.0.0",
    "http-status": "^1.5.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.27.0",
    "mongoose": "^7.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^4.0.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "^4.1.4",
    "validator": "^13.0.0",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.6",
    "@types/express-fileupload": "^1.4.1",
    "@types/hapi__joi": "^17.1.3",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.194",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.16",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-facebook-token": "^0.4.35",
    "@types/passport-jwt": "^3.0.8",
    "@types/recaptcha2": "^1.3.1",
    "@types/supertest": "^2.0.10",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-adjunct": "^4.12.2",
